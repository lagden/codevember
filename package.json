  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.3",
    "grunt-browser-sync": "^2.1.3",
    "grunt-concurrent": "^2.0.4",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-jade": "^0.15.0",
    "grunt-contrib-stylus": "^0.22.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-critical": "^0.1.7",
    "grunt-postcss": "^0.7.0",
    "grunt-xo": "^0.6.0",
    "jeet": "^6.1.2",
    "jit-grunt": "^0.9.1",
    "nib": "^1.1.0",
    "time-grunt": "^1.2.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "xo": {
    "envs": [
      "es6",
      "browser"
    ],
    "esnext": true,
    "ignores": [
      "build/**",
      "dist/**"
    ],
    "global": [
      "window",
      "document",
      "requirejs",
      "require",
      "define"
    ],
    "rules": {
      "global-require": 0,
      "quote-props": [
        2,
        "as-needed"
      ],
      "new-cap": [
        2,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ]
    }
  }
